<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean name="emp1" class="com.seed.model.Employee">
		<property name="id" value="1" />
		<property name="name" value="jack" />
		<property name="salary" value="5000" />
	</bean>

	<bean name="emp4" class="com.seed.model.Employee">
		<constructor-arg index="0" value="4" />
		<constructor-arg index="1" value="Jessica" />
		<constructor-arg index="2" value="50000" />
	</bean>

	<!-- <bean name="emp4_v1" class="com.seed.model.Employee">
		<constructor-arg name="id" value="4" />
		<constructor-arg name="name" value="Jessica" />
		<constructor-arg name="salary" value="50000" />
	</bean> -->


	<bean name="emp2" class="com.seed.model.Employee">
		<constructor-arg name="name" value="John" />
		<constructor-arg name="salary" value="1000" />
		<property name="id" value="2"></property>
	</bean>

	<bean name="emp3" class="com.seed.model.Employee">
		<constructor-arg name="id" value="4" />
		<constructor-arg name="name" value="Sam" />
		<property name="salary" value="8000"></property>
	</bean>

</beans>